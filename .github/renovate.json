{
    "extends": [
        "config:base",
        "schedule:weekly",
        ":disableDependencyDashboard"
    ],
    "ignoreDeps": [
        "google.golang.org/grpc"
    ],
    "semanticCommits": false,
    "postUpdateOptions": [
        "gomodTidy"
    ],
    "commitMessagePrefix": "chore(all): ",
    "commitMessageAction": "update",
    "labels": [
        "automerge"
    ],
    "groupName": "all",
    "force": {
        "constraints": {
            "go": "1.20"
        }
    }
}
